<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th style="width:20%;">xi</th>
      <th style="width:20%;">fi</th>
      <th style="width:20%;">fr%</th>
      <th style="width:20%;">F</th>
      <th style="width:20%;">F%</th>
    </tr>
  </thead>
  <tbody>
    <% @table.each do |row| %>
    <tr>
      <td><%= row[1]["xi"] %></td>
      <td><%= row[1]["fi"] %></td>
      <td><%= row[1]["fr%"] %>%</td>
      <td><%= row[1]["F"] %></td>
      <td><%= row[1]["F%"] %>%</td>
    </tr>
    <% end %>
  </tbody>
</table>